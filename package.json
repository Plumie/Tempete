{
  "name": "tempete",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "bin": {
    "tempete": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --no-stdin --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "serve": "tsc -w src/index.ts",
    "build": "tsc -p .",
    "local": "tsc -p . && npm i -g",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^6.1.0",
    "ejs": "^3.1.5",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "ora": "^5.1.0",
    "path": "^0.12.7",
    "three": "^0.121.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.3",
    "nodemon": "^2.0.6",
    "sass": "^1.27.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
